<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>


<!-- 	Dto객체의 변수 설정 -->
	<typeAliases>
		<typeAlias type="com.hk.conred.dtos.UDto" alias="UDto"/>
		<typeAlias type="com.hk.conred.dtos.ODto" alias="ODto"/>
		
		<typeAlias type="com.hk.conred.dtos.InterestsDto" alias="InterestsDto"/>
		<typeAlias type="com.hk.conred.dtos.ReserveDto" alias="ReserveDto"/>
		<typeAlias type="com.hk.conred.dtos.ReplyDto" alias="ReplyDto"/>
		<typeAlias type="com.hk.conred.dtos.QnaDto" alias="QnaDto"/>
		<typeAlias type="com.hk.conred.dtos.LikeDto" alias="LikeDto"/>
		
		<typeAlias type="com.hk.conred.dtos.SDto" alias="SDto"/>
		<typeAlias type="com.hk.conred.dtos.STimeDto" alias="STimeDto"/>
		<typeAlias type="com.hk.conred.dtos.SPhotoDto" alias="SPhotoDto"/>
		<typeAlias type="com.hk.conred.dtos.SLocaDto" alias="SLocaDto"/>
	</typeAliases>
 

  
   
  
  	<mappers>
    	<mapper resource="./sqls/UMapper.xml"/>
