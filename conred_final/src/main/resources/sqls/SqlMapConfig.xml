    	<mapper resource="./sqls/AMapper.xml"/>
    	
    	<mapper resource="./sqls/InterestsMapper.xml"/>   	
    	<mapper resource="./sqls/ReserveMapper.xml"/>
    	<mapper resource="./sqls/ReplyMapper.xml"/>
    	<mapper resource="./sqls/QnaMapper.xml"/>
    	<mapper resource="./sqls/LikeMapper.xml"/>
    	
    	<mapper resource="./sqls/SMapper.xml"/>
    	<mapper resource="./sqls/STimeMapper.xml"/>
    	<mapper resource="./sqls/SPhotoMapper.xml"/>
    	<mapper resource="./sqls/SLocaMapper.xml"/>
  	</mappers>
</configuration>

