<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
  
<mapper namespace="com.hk.conred.Reply">
	
	<select id="replyList" parameterType="String" resultType="ReplyDto">
		SELECT REPLY_SEQ,USER_ID,STORE_SEQ,REPLY_CONTENT,REPLY_CLEAN,REPLY_PRICE,REPLY_SERVICE,REPLY_ANSWER,REPLY_READ,REPLY_REGDATE,REPLY_DELFLAG
		FROM CON_REPLY
		WHERE USER_ID=#{user_id};
	</select>
	
	
</mapper>
	 